<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="mainPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.client.controller.MsgCell">
   <children>
      <VBox layoutY="-2.0">
         <children>
            <HBox spacing="10.0">
               <children>
                  <ImageView fx:id="senderImage" fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true" />
                  <Label fx:id="userName" text="ID" />
                  <Label fx:id="sendTime" text="time" />
               </children>
            </HBox>
            <Label fx:id="sendText" text="Label" />
         </children>
      </VBox>
   </children>
</AnchorPane>
