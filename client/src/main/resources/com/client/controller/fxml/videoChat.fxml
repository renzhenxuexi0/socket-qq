<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171">
    <children>
        <GridPane>
            <columnConstraints>
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0"/>
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <AnchorPane fx:id="myVideoPane" prefHeight="320.0" prefWidth="480.0" GridPane.columnIndex="1"
                            GridPane.rowIndex="1">
                </AnchorPane>
                <AnchorPane fx:id="talkVideoPane" prefHeight="320.0" prefWidth="480.0" GridPane.rowIndex="1"/>
                <Label fx:id="talkUsernameLabel" text="Label"/>
                <Label fx:id="myUsernameLabel" text="Label" GridPane.columnIndex="1"/>
            </children>
        </GridPane>
        <JFXButton fx:id="hangUpButton" graphicTextGap="0.0"/>
    </children>
</VBox>
