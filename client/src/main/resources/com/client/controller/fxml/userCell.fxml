<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<HBox alignment="CENTER_LEFT" prefHeight="45.0" prefWidth="280.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.client.controller.UserCell">
    <children>
        <ImageView fx:id="headImage" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true" />
        <VBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="200.0" style="-fx-spacing: 5;">
            <children>
                <Label fx:id="usernameLabel" text="name">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <Label fx:id="msgLabel" text="msg" textFill="#797575" />
            </children>
        </VBox>
        <Label fx:id="dataLabel" text="date">
         <font>
            <Font size="11.0" />
         </font></Label>
    </children>
</HBox>
