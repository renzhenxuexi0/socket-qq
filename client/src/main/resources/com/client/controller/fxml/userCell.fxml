<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.client.controller.UserCell" fx:id="mainPane"
            xmlns="http://javafx.com/javafx/8.0.171">
    <children>
        <Label fx:id="dateLabel" layoutX="228.0" layoutY="15.0" text="date">
            <font>
                <Font size="11.0"/>
            </font>
        </Label>
        <VBox alignment="CENTER_LEFT" layoutX="40.0" layoutY="2.0" style="-fx-spacing: 5;">
            <children>
                <Label fx:id="usernameLabel" text="name">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Label>
                <Label fx:id="msgLabel" text="textMsg" textFill="#797575"/>
            </children>
        </VBox>
        <ImageView fx:id="headImage" fitHeight="40.0" fitWidth="40.0" layoutY="2.0" pickOnBounds="true"
                   preserveRatio="true"/>
    </children>
</AnchorPane>
