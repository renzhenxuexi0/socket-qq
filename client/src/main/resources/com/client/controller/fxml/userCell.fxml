<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="mainPane" xmlns="http://javafx.com/javafx/8.0.171"
            fx:controller="com.client.controller.UserCell">
    <children>
        <HBox alignment="CENTER" spacing="60.0">
            <children>
                <Circle fx:id="stateCircle" fill="#cdcdcd" radius="12.0" stroke="BLACK" strokeType="INSIDE"/>
                <Label fx:id="usernameLabel" text="name" textFill="#1e1e1e">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Label>
            </children>
        </HBox>
    </children>
</AnchorPane>
